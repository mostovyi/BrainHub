:"src/components","sources":["EasyOneSeven.vue"],"sourcesContent":["<template>\n  <div>\n    <div>\n      <button @click=\"rangeOneTwo( repeats )\">Result is: {{ answer }}</button>\n    </div>\n    <div>\n      <div v-for=\"(element, i) in array\" :key=\" 'A'+ i\">{{ element }} </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"EasyOneSeven\",\n  data: function () {\n    return {\n      array: [],\n      answer: 0\n    }\n  },\n  // Input parameters\n  props: {\n    repeats : Number\n  },\n  // Digits generation function\n  methods: {\n    rangeOneTwo : function (repeats) {\n      let help = [];\n      let helpAnswer = 0;\n      let currentValue = 0;\n      for(let i = 0; i < repeats; i++){\n\n        switch (helpAnswer) {\n\n          case 0: {\n            currentValue = this.getRandomIntInclusive(1, 7);\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 1: {\n            let list1 = [1, 2, 3, 5, 6, 7, -1];\n            currentValue = this.getRandomIntInclusive(0, 6);\n            currentValue = list1[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 2: {\n            let list2 = [1, 2, 5, 6, 7, -1, -2];\n            currentValue = this.getRandomIntInclusive(0, 6);\n            currentValue = list2[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 3: {\n            let list3 = [1, 5, 6, -3, -2, -1];\n            currentValue = this.getRandomIntInclusive(0, 5);\n            currentValue = list3[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 4: {\n            let list4 = [5, -4, -3, -2, -1];\n            currentValue = this.getRandomIntInclusive(0, 4);\n            currentValue = list4[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 5: {\n            let list5 = [-5, 1, 2, 3, 4];\n            currentValue = this.getRandomIntInclusive(0, 4);\n            currentValue = list5[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 6: {\n            let list6 = [-6, -5, -1, 1, 2, 3];\n            currentValue = this.getRandomIntInclusive(0, 5);\n            currentValue = list6[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 7: {\n            let list7 = [-7, -6, -5, -2, -1, 1, 2];\n            currentValue = this.getRandomIntInclusive(0, 6);\n            currentValue = list7[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 8: {\n            let list8 = [-7, -6, -5, -3, -2, -1, 1];\n            currentValue = this.getRandomIntInclusive(0, 6);\n            currentValue = list8[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n          case 9: {\n            let list9 = [-7, -6, -5, -4, -3, -2, -1];\n            currentValue = this.getRandomIntInclusive(0, 6);\n            currentValue = list9[currentValue];\n            help.push(currentValue);\n            helpAnswer += currentValue;\n            break;\n          }\n        }\n      }\n\n      this.array = help;\n      this.answer = helpAnswer;\n    },\n    // Generates random values\n    getRandomIntInclusive : function (min, max) {\n    min = Math.ceil(min);\n    max = Math.floor(max);\n    return Math.floor(Math.random() * (max - min + 1)) + min; //Максимум и минимум включаются\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}